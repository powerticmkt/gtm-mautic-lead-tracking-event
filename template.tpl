___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
const readTitle = require('readTitle');
const getUrl = require('getUrl');
const encodeUriComponent = require('encodeUriComponent');
const sendPixel = require('sendPixel');
const makeTableMap = require('makeTableMap' );
let pixelURL = "";
// Monta a os parametros
pixelURL += "page_url=";
pixelURL += data.event_url == undefined?encodeUriComponent(getUrl()):encodeUriComponent(data.event_url);
pixelURL += "&page_title=";
pixelURL += data.event_identifier == undefined?encodeUriComponent(readTitle()):encodeUriComponent(data.event_identifier);
if( data.tags !== undefined ) {
  pixelURL += "&tags=" + encodeUriComponent(data.tags);
}
// processa os campos do contato
if( data.lead_fields !== undefined ) {
  const lead_fields = makeTableMap(data.lead_fields, "field_name","field_value");
  for( let key in lead_fields ){
    pixelURL += "&" + key +"=";
    pixelURL += encodeUriComponent(lead_fields[key]);
  }
}
// processa os utm
if( data.utm !== undefined ) {
  const utms = makeTableMap(data.utm, "name","value");
  for( let key in utms ){
    pixelURL += "&" + key +"=";
    pixelURL += encodeUriComponent(utms[key]);
  }
}
// monta o url
pixelURL = data.fqdn +"/mtracking.gif?" + pixelURL;
sendPixel(pixelURL);
data.gtmOnSuccess();


